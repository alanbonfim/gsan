<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gcom.cadastro.cliente.Cliente" table="cadastro.cliente">
	
		<id name="id" type="java.lang.Integer" column="clie_id">
			<generator class="sequence">
				<param name="sequence">cadastro.seq_cliente</param>
			</generator>
		</id>
		
		<property name="numeroNIS" 			type="java.lang.Integer" 	column="cadu_nnnis" 				length="11" />
		<property name="nome" 				type="java.lang.String" 	column="cadu_nmnome" 				length="50" />
		<property name="dataNascimento" 	type="java.sql.Date" 		column="cadu_dtnascimento" 			length="4" />
		<property name="ddd" 				type="java.lang.String" 	column="cadu_cdddd" 				length="2" />
		<property name="telefone" 			type="java.lang.String" 	column="cadu_nnfone" 				length="9" />
		<property name="cpf" 				type="java.lang.String" 	column="cadu_nncpf" 				length="11" />
		<property name="dataCadastramento" 	type="java.sql.Date" 		column="cadu_dtcadastramento" 		length="4" />
		<property name="municipio" 			type="java.lang.String" 	column="cadu_nmmunicipiouf" 		length="30" />
		<property name="ultimaAlteracao" 	type="java.sql.Timestamp" 	column="cadu_tmultimaalteracao" 	length="8" />
		<property name="dataInclusao" 		type="java.sql.Timestamp" 	column="cadu_tminclusao" 			length="8" />
		<property name="dataExclusao" 		type="java.sql.Timestamp" 	column="cadu_tmexclusao" 			length="8" />

		<!-- bi-directional many-to-one association to PessoaSexo -->
		<many-to-one name="pessoaSexo"
			class="gcom.cadastro.cliente.PessoaSexo">
			<column name="psex_id" />
		</many-to-one>
		
		<!-- bi-directional many-to-one association to FoneTipo -->
		<many-to-one name="foneTipo" class="gcom.cadastro.cliente.FoneTipo">
			<column name="fnet_id"/>
		</many-to-one>
		
	</class>
</hibernate-mapping>
